{
  "name": "psedit",
  "version": "1.1.2",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "edit processes list",
  "homepage": "http://github.com/coderaiser/node-psedit",
  "main": "lib/psedit.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-psedit.git"
  },
  "keywords": [
    "process",
    "kill",
    "edit",
    "ps"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "lint": "redrun lint:*",
    "lint:lib": "eslint -c .eslintrc.server lib",
    "lint:server": "eslint test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "currify": "^2.0.6",
    "format-io": "^1.0.1",
    "systeminformation": "^3.42.0",
    "table": "^4.0.3"
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-plugin-node": "^7.0.1",
    "mock-require": "^3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^12.0.1",
    "redrun": "^6.0.1",
    "sinon": "^6.0.0",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
